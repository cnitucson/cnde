{
  "name": "Geomech Data Entry",
  "description": "Enter and verify geomechanical data",
  "version": "1.1.4",
  "manifest_version": 2,
  "app": {
    "launch": {
      "local_path": "index.html"
    }
  },
  "icons": {
    "16": "16.png",
    "48": "48.png",
    "128": "128.png"
  },
  "offline_enabled": true,
  "permissions": [
    "unlimitedStorage"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
